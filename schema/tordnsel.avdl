@namespace("org.torproject.metrics.converTor")
protocol tordnsel_descriptor {

  record ExitNode {
    union { null, string } fingerprint;
    union { null, string } published;
    union { null, string } last_status;
    union { null, map<string> } exit_list;
  }

  @aliases(["exitList"])
  record Tordnsel {

    string descriptor_type = "tordnsel 1.0";
    union { null, string } downloaded;
    union { null, array<ExitNode> } exit_nodes;

  }

}
